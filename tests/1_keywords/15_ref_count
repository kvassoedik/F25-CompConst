type House is record
	var a: integer
	var b: integer
end

type Arr is array[5] real

routine func(h: House): House is
    var h3 is h
    print h3.a
    return h
end

routine main() is
    var h1: House
    var h2 is h1
    h1.a := 2
    h1.b := 7

    var i is func(h2).b // prints 2
    h1.b := 9
    print i // 7
    print h1.b // 9
end