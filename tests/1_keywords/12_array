type intArr is array[5] integer

var X: intArr
var Y: array[2] integer

routine main() is
	var i is 0
	while i < 5 loop
		X[i] := i
		if i < 2 then
			Y[i] := i+10
		end
		i := i + 1
	end

	i := 0
	while i < 5 loop
		print X[i]
		if i < 2 then
			print Y[i]
		end
		i := i + 1
	end
end

/* Output:
0 10 1 11 2 3 4
*/