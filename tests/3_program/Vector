type Vector is record
    var size: integer is 0
    var reserved: integer is 2
    var arr: array[2] integer
end

routine push(v: Vector, val: integer): boolean is
    if v.size + 1 > v.reserved then
        return false
    end
    v.size = v.size + 1
    v.arr[v.size] := val

    return true
end

routine pop(v: Vector): boolean is
    if v.size = 0 then
        return false
    end
    v.size = v.size - 1
    v.arr[v.size] := 0

    return true
end

var v: Vector
print push(v, 10) // true
print push(v, -15) // true
print push(v, 25) // false
print
print pop(v) // true
for i in v loop print v.arr[i] end // 10 0
print pop(v) // true
print pop(v) // false